<?xml version='1.0' encoding='utf-8'?>
<widget id="com.linegenix.app" version="0.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Linegenix</name>
    <description>Manage waiting lines  from your smartphone and your clients'.</description>
    <access origin="*" />
    <allow-navigation href="*://*.mendix.com/*" />
    <allow-navigation href="*://*.mendixcloud.com/*" />
    <allow-navigation href="*://*.mxapp.io/*" />
    <allow-navigation href="about:*" />
    <splash src="res/android/screen-xxxhdpi-portrait.png" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
